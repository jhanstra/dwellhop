<h1 class="centered top-header"><%= yield(:header) %></h1>
  <div class="row">
    <div class="large-6 centered large-offset-3">
      <%= render 'shared/error_messages_home' %>

      <%= form_for(@home) do |f| %>

          <%= f.label :address %>
          <%= f.text_field :address %>

          <%= f.label :city %>
          <%= f.text_field :city %>


          <%= f.label :zip_code %>
          <%= f.text_field :zip_code %>

          <%= f.label :list_price %>
          <%= f.text_field :list_price %>      


        <%= f.submit yield(:buttontext), class: "button small my-button radius" %>
      <% end %>
    </div>
  </div>
